<manifest xmlns:android="http://schemas.android.com/apk/res/android"          package="com.seuic.nfctest" >    <uses-permission android:name="android.permission.NFC" />    <!-- 模块中要用到四大组件的话，需要先在这里注册-->    <uses-feature            android:name="android.hardware.nfc"            android:required="true" />    <application            android:allowBackup="true"            android:label="@string/app_name"            android:supportsRtl="true">        <!--背景透明-->        <!--android:theme="@android:style/Theme.Translucent.NoTitleBar"-->        <!--简单无标题的-->        <!--android:theme="@android:style/Theme.NoTitleBar"-->        <!-- 自己写的两个测试Activity-->        <activity                android:name="com.epichust.nfc_2023.NFCReadActivity"                android:launchMode="singleTop"                android:theme="@android:style/Theme.Translucent.NoTitleBar"/>        <activity                android:name="com.epichust.nfc_2023.NFCWriteActivity"                android:launchMode="singleTop"                android:theme="@android:style/Theme.Translucent.NoTitleBar"/><!--        android:theme="@style/TransparentTheme"-->        <!-- 源代码中Activity-->        <activity                android:name=".ReadTextActivity"                android:launchMode="singleTop">            <!--<intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>-->        </activity>        <activity                android:name=".WriteTextActivity"                android:launchMode="singleTop"/>    </application></manifest>